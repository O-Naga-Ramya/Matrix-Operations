//Transpose of a given matrix of any order
#include <stdio.h>

int main()
{	printf("Transpose of a matrix");
    int i,j,k,l;
    int matrix[10][10],matrix1[10][10];
    printf ("enter no of rows");
    scanf("%d", &i);
    printf ("enter no of columns");
    scanf("%d", &j);
      for (k=1;k<=i;k++)
        {for(l=1;l<=j;l++)
            {printf ("enter the element%d%dplace",k,l);
            scanf("%d", &matrix[k][l]);
            }
        }
        printf("entered matrix");
        printf("\n");
        for (k=1;k<=i;k++)
        	{for(l=1;l<=j;l++)
            {printf ("%d\t",matrix[k][l]);
            }
            printf("\n");
        }
    //finding transpose of a matrix
    for(k=1;k<=j;k++)
		{for(l=1;l<=i;l++)
			{matrix1[k][l]=matrix[l][k];
			}
		}
		printf("transpose of given matrix");
		printf("\n");
		for (k=1;k<=j;k++)
        	{for(l=1;l<=i;l++)
            	{printf ("%d\t",matrix1[k][l]);}
            printf ("\n");
        }
    }
